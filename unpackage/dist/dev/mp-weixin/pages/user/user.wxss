.saleCenter_txt {
  font-size: 24rpx;
  color: #cccccc;
  text-align: center;
}
.saleCenter_pic {
  width: 100%;
  text-align: center;
  margin: 30rpx 0;
}
.saleCenter_pic image {
  width: 80px;
  height: 80px;
}
.serviceItem_pic image {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 10rpx;
}
.serviceItem:nth-of-type(3n) {
  border-right: 0 !important;
}
.serviceArea {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin: 10px auto;
  background-color: #ffffff;
  text-align: center;
}
.serviceArea .serviceItem {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 33%;
          flex: 0 0 33%;
  border-bottom: 1px solid #f0f0f0;
  border-right: 1px solid #f0f0f0;
  padding: 40rpx 0;
}
.serviceArea .serviceItem .serviceItem_name {
  font-size: 28rpx;
}
.saleCenter {
  width: 100%;
  background-color: #ffffff;
  margin: 0 auto 30rpx auto;
  padding: 15rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.saleCenter .saleCenterTitle {
  font-size: 30rpx;
  font-weight: 600;
  padding: 10rpx 0;
  border-bottom: 1px solid #f0f0f0;
}
.saleCenter .saleCenterTitle .myOrderMore {
  font-size: 24rpx;
  font-weight: 100;
}
.myOrderArea {
  width: 100%;
  background-color: #ffffff;
  padding: 6px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.myOrderArea .myOrderTop {
  font-size: 30rpx;
  font-weight: 600;
  padding: 10rpx 0;
  border-bottom: 1px solid #f0f0f0;
}
.myOrderArea .myOrderTop .myOrderMore {
  font-size: 24rpx;
  font-weight: 100;
}
.myOrderArea .myOrderDown {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  text-align: center;
}
.myOrderArea .myOrderDown .myOrderCon {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 20%;
          flex: 0 0 20%;
  font-size: 24rpx;
  margin: 15rpx 0;
}
.myOrderCon image {
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
}
.topBgColor {
  background: -webkit-gradient(linear, left top, left bottom, from(#FEB405), to(#FFC63A));
  background: -webkit-linear-gradient(top, #FEB405, #FFC63A);
  background: linear-gradient(to bottom, #FEB405, #FFC63A);
  width: 100%;
  height: 200px;
  position: fixed;
  top: 0;
  z-index: 99;
}
.headIconArea {
  width: 100%;
  margin: 120rpx auto 0 auto;
  text-align: center;
  position: relative;
}
.headIconArea .headIcon {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 10%;
          flex: 0 0 10%;
}
.headIconArea .headIcon image {
  width: 60px;
  height: 60px;
  border-radius: 100%;
}
.headIconArea .headIcontxt {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 60%;
          flex: 0 0 60%;
  color: #ffffff;
}
.headIconArea .headIcontxt .personalName {
  font-size: 16px;
  margin: 10rpx 0;
  color: #323232;
}
.headIconArea .headIcontxt .personalTitle {
  width: 50px;
  padding: 2rpx;
  font-size: 20rpx;
  background-color: rgba(0, 0, 0, 0.3);
  border-radius: 200rpx;
  text-align: center;
  margin: 10px auto;
}
.headIconArea > image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 20rpx;
}
.headIconArea > .setBtn {
  right: 100rpx;
}
